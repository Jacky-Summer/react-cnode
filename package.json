{
  "name": "react-cnode",
  "version": "1.0.0",
  "description": "React SSR服务器端渲染制作社区WebApp",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build:client": "webpack --mode production --config build/webpack.config.client.js",
    "build:server": "webpack --mode production --config build/webpack.config.server.js",
    "dev:client": "cross-env NODE_ENV=development webpack-dev-server --config build/webpack.config.client.js",
    "dev:server": "nodemon server/server.js",
    "clear": "rimraf dist",
    "build": "npm run clear && npm run build:client && npm run build:server",
    "start": "node server/server.js",
    "lint": "eslint --ext .jsx --ext .js client/",
    "precommit": "npm run lint",
    "commit": "git-cz"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "axios": "^0.19.2",
    "babel-loader": "^8.1.0",
    "express": "^4.17.3",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "serve-favicon": "^2.5.0",
    "webpack": "^4.43.0"
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/preset-env": "^7.9.5",
    "@babel/preset-react": "^7.9.4",
    "babel-core": "^6.26.3",
    "babel-eslint": "^10.1.0",
    "commitizen": "^4.0.4",
    "cross-env": "^7.0.2",
    "cz-conventional-changelog": "^3.1.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-standard": "^4.0.1",
    "html-webpack-plugin": "^4.2.1",
    "http-proxy-middleware": "^1.0.3",
    "husky": "^4.2.5",
    "memory-fs": "^0.5.0",
    "nodemon": "^2.0.3",
    "prettier": "^2.0.5",
    "react-hot-loader": "^4.5.3",
    "rimraf": "^3.0.2",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3",
